{
  "name": "@1studio/kontakt2",
  "version": "0.0.0-semantically-released",
  "description": "Kontakt 2",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build && mkdir build && ls -p ./src | grep -v / | grep -v '.jsx\\?$' | xargs -I '$1' cp ./src/'$1' ./build/",
    "validate": "npm run validate:lint && npm run validate:coverage",
    "validate:lint": "node_modules/.bin/eslint src",
    "validate:flow": "flow",
    "prevalidate:coverage": "npm run test:coverage",
    "validate:coverage": "nyc check-coverage --statelments 90 --branches 80 --functions 100 --lines 90",
    "report:coverage": "cat ./coverage/lcov.info | codecov",
    "start": "webpack-dev-server --open --progress --hot --color --env.dev",
    "test": "mocha",
    "test:watch": "npm t -- -w",
    "test:debug": "npm t -- -w --inspect-brk",
    "test:coverage": "nyc npm t && nyc report --reporter=lcov",
    "docs": "node_modules/.bin/documentation build ./src/index.js -f md -o MANUAL.md --github",
    "docs:view": "node_modules/.bin/documentation serve --watch ./src/index.js",
    "__prebuild": "npm run clean",
    "__prebuild:prod": "npm run clean",
    "build": "webpack --env.dev",
    "build:prod": "webpack --env.prod -p",
    "build:debug": "webpack --display-error-details",
    "prewatch": "npm run build",
    "update": "npm i -S @1studio/ui@latest && npm i -S @1studio/utils@latest",
    "watch": "node_modules/.bin/webpack-dev-server --env.dev"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "admin"
  ],
  "author": "Robert Gulyas <robert@1studio.hu> (http://www.1studio.hu/)",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "autoprefixer": "9.3.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.0.0",
    "babel-minify": "0.2.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "^6.3.13",
    "babel-register": "6.26.0",
    "babel-watch": "2.0.7",
    "benchmark": "2.1.4",
    "chai": "^4.0.2",
    "config-yml": "0.8.0",
    "core-js": "2.5.3",
    "css-loader": "0.28.7",
    "deep-freeze": "0.0.1",
    "documentation": "5.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "2.1.2",
    "html-webpack-plugin": "2.30.1",
    "js-yaml": "3.10.0",
    "mocha": "^3.4.2",
    "node-sass": "4.7.2",
    "nyc": "^11.0.3",
    "postcss-inline-svg": "3.0.0",
    "postcss-loader": "2.0.9",
    "pug": "2.0.0-rc.4",
    "pug-loader": "2.3.0",
    "rimraf": "^2.6.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "transform-loader": "0.2.4",
    "uglify-js": "3.6.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "2.9.3",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "@1studio/ui": "1.13.0",
    "@1studio/utils": "4.4.0",
    "blob-stream": "0.1.3",
    "classnames": "2.2.5",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "pdfkit": "0.8.3",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-intl": "2.4.0",
    "react-redux": "5.0.5",
    "react-router-dom": "4.2.2",
    "react-router-props": "1.0.2",
    "redux": "3.7.2",
    "superagent": "4.0.*",
    "svg-to-pdfkit": "0.1.7"
  }
}
