{
  "name": "@1studio/ui",
  "version": "1.0.0",
  "description": "A collection of useful database handling React Redux UI.",
  "main": "src/index.js",
  "scripts": {
    "____clean": "rimraf build && mkdir build && cp examples/*.json build/ && cp examples/index.html build/",
    "clean": "cp examples/*.json build/ && cp examples/index.html build/",
    "validate": "npm run validate:lint && npm run validate:coverage",
    "validate:lint": "node_modules/.bin/eslint src",
    "validate:flow": "flow",
    "prevalidate:coverage": "npm run test:coverage",
    "validate:coverage": "nyc check-coverage --statements 90 --branches 80 --functions 100 --lines 90",
    "test": "mocha",
    "test:watch": "npm t -- -w",
    "test:debug": "npm t -- -w --inspect-brk",
    "test:coverage": "nyc npm t",
    "docs": "node_modules/.bin/documentation build ./utils/manual.js -f md -o MANUAL.md --github",
    "docs:view": "node_modules/.bin/documentation serve --watch ./src/index.js",
    "docs:react": "rimraf docs && mkdir docs && react-docgen ./src/grid/pure/*.jsx --pretty -o ./docs/docs.json && cat ./docs/docs.json | ./utils/buildDocs.sh",
    "example:icon": "find src/icon/mui | ./utils/buildIcons.sh && find src/icon/la | ./utils/buildIcons.sh",
    "#precommit": "npm run validate",
    "commit": "git-cz",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roberto404/ui.git"
  },
  "keywords": [
    "ui",
    "react",
    "redux",
    "components",
    "grid",
    "table",
    "form",
    "chart"
  ],
  "author": "Robert Gulyas <robert@1studio.hu> (http://www.1studio.hu/)",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/roberto404/ui/issues"
  },
  "homepage": "https://github.com/roberto404/ui#readme",
  "devDependencies": {
    "autoprefixer": "9.3.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "7.1.2",
    "babel-minify": "0.2.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "^6.3.13",
    "babel-register": "6.26.0",
    "babel-watch": "2.0.7",
    "benchmark": "2.1.4",
    "chai": "^4.0.2",
    "commitizen": "2.9.6",
    "core-js": "2.5.3",
    "cz-conventional-changelog": "2.1.0",
    "deep-freeze": "0.0.1",
    "documentation": "5.3.3",
    "enzyme": "2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "0.57.3",
    "jsdom": "11.3.0",
    "mocha": "4.0.1",
    "node-sass": "4.7.2",
    "nyc": "^11.0.3",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "15.6.1",
    "rimraf": "^2.6.1",
    "semantic-release": "15.13.12",
    "semantic-release-cli": "3.6.3",
    "sinon": "4.1.2"
  },
  "dependencies": {
    "@1studio/utils": "4.2.0",
    "@tinymce/tinymce-react": "2.5.0",
    "classnames": "2.2.5",
    "draft-js": "0.10.5",
    "draftjs-to-html": "0.8.4",
    "dropzone": "5.3.0",
    "file-saver": "2.0.0",
    "hammerjs": "2.0.8",
    "history": "4.7.2",
    "html-to-draftjs": "1.4.0",
    "intl": "1.2.5",
    "jquery": "3.4.1",
    "lodash": "4.17.11",
    "material-ui": "0.18.7",
    "moment": "2.24.0",
    "node-uuid": "1.4.8",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-draft-wysiwyg": "1.12.13",
    "react-intl": "2.4.0",
    "react-list": "0.8.11",
    "react-redux": "5.0.5",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-promise": "0.5.3",
    "superagent": "3.8.0",
    "validate.js": "0.11.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
