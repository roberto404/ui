{
  "name": "@1studio/rs",
  "version": "1.0.0",
  "description": "RS",
  "main": "src/index.js",
  "scripts": {
    "#precommit": "npm run validate",
    "commit": "git-cz",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
